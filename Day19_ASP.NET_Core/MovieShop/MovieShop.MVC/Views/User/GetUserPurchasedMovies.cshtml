@model ApplicationCore.Models.Response.PurchaseResponseModel

@{
    ViewData["Title"] = "UserPurchasedMovies";
}

<div class="rounded">
    <div class="container-fluid bg-light">
        <div class="row">
            @foreach (var movie in Model.PurchasedMovies)
            {
                <div class="col-6 col-lg-3 col-sm-4 col-xl-2">

                    <partial name="_MovieCard" model="movie" />
                </div>
            }
        </div>
    </div>
</div>